{
  "name": "surrealql",
  "displayName": "SurrealQL",
  "description": "Official support for the surrealql query language, used by SurrealDB!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "surrealql",
      "aliases": ["SurrealQL", "surrealql"],
      "extensions": [".surql",".surrealql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [
      {
        "language": "surrealql",
        "scopeName": "source.surrealql",
        "path": "./syntaxes/surrealql.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript"
        }
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.vue",
          "text.html.markdown",
          "source.surrealql"
        ],
        "scopeName": "inline.surrealql-js-literal",
        "path": "./syntaxes/surrealql-js-literal.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.surrealql": "surrealql"
        }
      }
    ],
    "snippets": [
      {
        "language": "surrealql",
        "path": "./snippets.json"
      }
    ]
  }
}
